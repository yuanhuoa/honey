{
  "name": "Honey-like",
  "short_name": "Honey-like",
  "options_page": "options.html",
  "version": "0.0.1",
  "version_name": "0.0.1",
  "description": "",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
